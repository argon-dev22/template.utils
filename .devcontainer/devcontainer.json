{
  "name": "dev", // any devcontainer name
  "service": "workspace", // service name in docker-compose.yml
  "workspaceFolder": "/workspace", // WORKDIR in Dockerfile
  "dockerComposeFile": "../docker-compose.dev.yml", // docker-compose.yml file path
  "customizations": {
    "vscode": {
      "extensions": [
        // Add extension's identifiers here
        "ms-azuretools.vscode-docker",
        "editorconfig.editorconfig",
        "cweijan.vscode-database-client2",
        "cweijan.dbclient-jdbc",
        "streetsidesoftware.code-spell-checker",
        "golang.go",
        "ms-python.python"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:1": {},
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers/features/go:1": {
      "version": "1.23"
    },
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.9"
    }
  }
}
